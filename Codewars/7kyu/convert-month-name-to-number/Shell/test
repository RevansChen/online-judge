describe "Solution for parameter 'Aug'" do
    it "should print proper output" do
        output = run_shell args: ['Aug']
        expect(output).to eq("08")
    end
end

describe "Solution for parameter 'Apr'" do
    it "should print proper output" do
        output = run_shell args: ['Apr']
        expect(output).to eq("04")
    end
end

